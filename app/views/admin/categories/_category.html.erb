<tr>
  <td>
    <h4>
      <%= category.name %>
    </h4>
  </td>
  <td>
    <h4>
      <%= time_ago_in_words(category.created_at) %> ago
    </h4>
  </td>
  <td>
    <h4>
      <%= category.products.length() %>
    </h4>
  </td>
  <td>
    <%= link_to fa_icon('trash'), [:admin, category], class: 'btn btn-sm btn-danger', method: :delete, data: { confirm: "Are you sure?" } %>
  </td>

</tr>